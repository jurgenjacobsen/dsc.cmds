{
  "name": "dsc.cmds",
  "version": "3.0.1",
  "description": "A slash command and context menu commands handler for Discord.js bots",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "docs": "typedoc --out docs src/",
    "pretty": "prettier --config .prettierrc \"./src/**/*.ts\"  \"./lib/**/*.js\" --write",
    "tsc": "tsc",
    "build": "npm run tsc && npm run pretty && npm run docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jurgenjacobsen/dsc.cmds.git"
  },
  "keywords": [
    "commands",
    "handler",
    "discord",
    "discord.js",
    "slash",
    "command"
  ],
  "author": "jurgenjacobsen",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/jurgenjacobsen/dsc.cmds/issues"
  },
  "engines": {
    "node": ">=18.12.1",
    "npm": ">=9.2.0"
  },
  "homepage": "https://github.com/jurgenjacobsen/dsc.cmds#readme",
  "dependencies": {
    "@discordjs/builders": "^1.4.0",
    "discord.js": "^14.7.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "prettier": "^2.8.1",
    "typedoc": "^0.23.23"
  }
}
